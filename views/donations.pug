extends ./index.pug

mixin css(src)
	-src = 'CSS/' + src
	link(href=src, rel="stylesheet", type="text/css")
mixin script(src)
	-src = 'JS/' + src
	script(type="text/javascript", src=src)

block pageName
	|Donations

block title
	h1 Donations
	h3 This page is a work in progress. It does not currently work.

block head
	+css('About.css')
	+script('About.js')

block content
	.textBlock
		h3 Nothing Yet
		| I have no text here yet